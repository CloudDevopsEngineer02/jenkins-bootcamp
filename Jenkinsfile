pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    
        stage('Hello-2') {
            steps {
                echo 'Hello World-2'
            }
        }
        
        stage('Hello-3') {
            steps {
                echo 'Hello World -3 '
            }
        }
        
        stage('Hello-4') {
            steps {
                echo 'Hello World - 4'
            }
        }
        
    }
}
